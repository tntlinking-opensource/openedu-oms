[#ftl]
<style>
	.num_ul .live-box-font1 span {letter-spacing: 5px;}
</style>
<ul class="live-box num_ul" id="live-box">
	<li style="margin-right: 2%;">
		<p style="font-family:digifacewide; color:#f3a909;">${completeNum}</p>
		<p class="live-box-font1"><span style="font-size:1.6rem;">已报到人数</span></p>
	</li>
	<li>
		<p style="font-family:digifacewide; color:#f33358;">${unCompleteNum}</p>
		<p class="live-box-font1"><span style="font-size:1.6rem;">未报到人数</span></p>
	</li>
</ul>
