				        $("select.jt_city").data("title", "市").dormCascadeBuilding(".jt_county").data("value", "${(studentHome.city.id)!}");
				        $(".jt_county").data("title", "县").data("value", "${(studentHome.county.id)!}");
				    });
				</script>
			[#else]
	            ${(studentHome.city.parent.name)!}${(studentHome.city.name)!}${(studentHome.county.name)!}
	        [/#if]
		</div>
	[#elseif property == "student.county"]
	[#--通讯地址--]
		<div class="col-sm-12" style="line-height: 30px;padding:0px;">
			<div class="col-sm-3" style="text-align: right;">
		    	[#if config.editable && config.required]<span class="requried">*</span>[/#if]
		    ${(config.name)!}
			</div>
			[#if config.editable]
				[#assign validateClass = validateClass + " tx_city"/]
				<div class="col-sm-3" style="text-align: left;">
					[@fu.select name="" value=(student.county.id)!0 data=[] class="tx_pro" /]
				</div>
				<div class="col-sm-3">
					[@fu.select name="student.city.id" value=(student.city.id)!0 data=[] class=validateClass  /]
				</div>
				<div class="col-sm-3" >
					[@fu.select name="student.county.id" value=(student.county.id)!0 data=[] class="tx_county" /]
				</div>
				<script>
				    $(function () {
				        $("select.tx_pro").data("title", "省").dormLoadCampus(".tx_city").data("value", "${(student.city.parent.id)!}");
				        $("select.tx_city").data("title", "市").dormCascadeBuilding(".tx_county").data("value", "${(student.city.id)!}");
				        $(".tx_county").data("title", "县").data("value", "${(student.county.id)!}");
				    });
				</script>
			[#else]
	            ${(student.city.parent.name)!}${(student.city.name)!}${(student.county.name)!}
	        [/#if]
		</div>
	[#elseif property == "student.enrollProvince"]
		[#--何省市报考--]
		[#--研究生--]
			<div class="col-sm-6" style="line-height: 30px; min-height: 34px;">
				<div class="col-sm-6" style="text-align: right;">
			    	[#if config.editable && config.required]<span class="requried">*</span>[/#if]
			    ${(config.name)!}
				</div>
				<div class="col-sm-6">
					[#if config.editable]
						[#assign validateClass = validateClass + " sh_city"/]
						[@fu.select name="student.enrollProvince.id" value=(student.enrollProvince.id)!0 data=[] class=validateClass /]
						<script>
						    $(function () {
						        $("select.sh_city").data("title", "省").dormLoadCampus(".sh_county").data("value", "${(student.enrollProvince.id)!}");
						    });
						</script>
					[#else]
			            ${(student.enrollProvince.name)!}
			        [/#if]
			        [#--<input type="button" class="btn btn-primary" value="说明" id="remark" style="width:50px"/>--]
		        </div>
		     </div>
	
	[#elseif property == "student.nativePlaceStr"]
    [#--籍贯--]
    	<div class="col-sm-6" style="line-height: 30px; min-height: 34px;">
			<div class="col-sm-6" style="text-align: right;">
		    	[#if config.editable && config.required]<span class="requried">*</span>[/#if]
		    ${(config.name)!}
			</div>
			[#if config.editable]
				[#assign validateClass = validateClass + " natCity"/]
				<div class="col-sm-6" style="text-align: left;">
					[@fu.select name="student.nativePlaceStr.id" value=(student.nativePlaceStr.id)!0 data=[] class="natPro" /]
				</div>
				</div>
				
    			<div class="col-sm-6" style="line-height: 30px; min-height: 34px;">
					<div class="col-sm-6">
						[@fu.select name="student.nativePlaceCity.id" value=(student.nativePlaceCity.id)!0 data=[] class=validateClass  /]
					</div>
					<div class="col-sm-6">
					</div>
					<script>
					    $(function () {
					        $("select.natPro").data("title", "省").nativeLoadPro(".natCity").data("value", "${(student.nativePlaceStr.id)!}");
					        $("select.natCity").data("title", "市").data("value", "${(student.nativePlaceCity.id)!}");
					    });
					</script>
			[#else]
				<div class="col-sm-6">
	            	${(student.nativePlaceStr.name)!}${(student.nativePlaceCity.name)!}
	            </div>
	        [/#if]
	      </div>
	      
	[#elseif property == "student.address"]
	[#--户口详细地址--]
		<div class="col-sm-12" style="line-height: 30px; min-height: 34px;">
			<div class="col-sm-3" style="text-align: right;">
				[#if config.editable && config.required]<span class="requried">*</span>[/#if]${(config.name)!}
			</div>
			<div class="col-sm-9" style="padding-left:8px;">
		    	<input  style="width:450px;" name="${config.property.property}" value="${text(config.property.property)!}"
				maxlength="${config.property.length!30}" [#if config.required]required[/#if]>
			</div>
		</div>
	[#elseif property == "studentHome.address"]
	[#--户口详细地址--]
		<div class="col-sm-12" style="line-height: 30px; min-height: 34px;">
			<div class="col-sm-3" style="text-align: right;">
				[#if config.editable && config.required]<span class="requried">*</span>[/#if]${(config.name)!}
			</div>
			<div class="col-sm-9" style="padding-left:8px;">
		    	<input  style="width:450px;" name="${config.property.property}" value="${text(config.property.property)!}"
				maxlength="${config.property.length!30}" [#if config.required]required[/#if]>
			</div>
		</div>
	[#else]
		<div class="col-sm-6" style="line-height: 30px; min-height: 34px;">
			<div class="col-sm-6" style="text-align: right;">
		        [#if config.editable && config.required]<span class="requried">*</span>[/#if]
		    ${(config.name)!}
			</div>
			<div class="col-sm-6">
		        [#if config.editable]
		            [#if property == "student.birthday"]
		            [#--生日--]
						<input name="student.birthday" value="${(student.birthday?date)!}" class="datepicker ${validateClass}" />
		            [#elseif property == "studentInfo.politicalStatus"]
		            [#--政治面貌--]
		                [@fu.select name="studentInfo.politicalStatus.id" value=(studentInfo.politicalStatus.id)!0 data=politicals class=validateClass /]
		            [#elseif property == "student.countries"]
		            [#--国家地区--]
		                [@fu.select2 name="student.countries.id" value=(student.countries.id)!0 data=gjdqs class=validateClass id="countries" /]
		            [#elseif property == "student.sourceType"]
		            [#--考生来源--]
		                [@fu.select name="student.sourceType.id" value=(student.sourceType.id)!0 data=sourceTypes class=validateClass /]
		            [#elseif property == "student.studentType" ]
					[#--学生类别--]
			    		[@fu.select name="student.studentType.id" value=(student.studentType.id)!0 data=studentTypes class=validateClass /]
		            [#elseif property == "studentInfo.maritalStatus"]
		            [#--婚姻状况--]
		                [@fu.select name="studentInfo.maritalStatus.id" value=(studentInfo.maritalStatus.id)!0 data=hyzks class=validateClass /]
		            [#elseif property == "studentInfo.language" ]
					[#--外语语种--]
				   		[@fu.select name="studentInfo.language" value=(studentInfo.language)! data=wyyzs valueKey="name" class=validateClass  /]
				    [#elseif property == "studentInfo.languageLevel" ]
				    [#--外语水平--]
   						[@fu.select name="studentInfo.languageLevel.id" value=(studentInfo.languageLevel.id)!0 data=wysps class=validateClass  /]
		            [#elseif property == "studentInfo.computerLevel"]
				    [#--计算机水平--]
				    	[@fu.select name="studentInfo.computerLevel.id" value=(studentInfo.computerLevel.id)!0 data=computerLevels class=validateClass style="width:150px;" /]
		            [#elseif property == "studentInfo.nation"]
		            [#--民族--]
		                [@fu.select name="studentInfo.nation.id" value=(studentInfo.nation.id)!0 data=nations class=validateClass /]
		            [#elseif property == "student.faith"]
		            [#--宗教信仰--]
		                [@fu.select name="student.faith.id" value=(student.faith.id)!0 data=faiths class=validateClass /]
		            [#elseif property == "studentInfo.health"]
		            [#--健康状况--]
		                [@fu.select name="studentInfo.health.id" value=(studentInfo.health.id)!0 data=healths class=validateClass /]
		            [#elseif property == "studentInfo.hmto"]
		            [#--是否澳台--]
		                [@fu.radios name="studentInfo.hmto" value=(studentInfo.hmto?string("1", "0"))!"0"
		                	data=[{"id":"1", "name":"是"}, {"id":"0", "name":"否"}] class=validateClass/]
		            [#elseif property == "student.isFullTime"]
		            [#--是否全日制--]
		                [@fu.radios name="student.isFullTime" value=(student.isFullTime?string("1", "0"))!"0"
		                	data=[{"id":"1", "name":"是"}, {"id":"0", "name":"否"}] class=validateClass/]
		            [#elseif property == "student.degreeType"
		            ||  property == "student.department"
		            ||  property == "student.major"
		            ||  property == "student.direction"
		            ||  property == "student.adminClass"
		            ||  property == "student.enrollmentDate"
		            ||  property == "student.duration"
		            ||  property == "student.campus"
		            ||  property == "student.gender"]
		                [@propertyInfoValue config/]
		            [#elseif property == "student.oneCardStatus"]
				    [#--一卡通办理状态--]
				        [@fu.select name="student.oneCardStatus" value=(student.oneCardStatus?string("1", "0"))!"0" 
				        	data=[{"id":"1", "name":"已办理"}, {"id":"0", "name":"未办理"}] class=validateClass/]
		            [#elseif property == "student.cardType" ]
				    [#--证件类型--]
				    	[@fu.select name="student.cardType" value=(student.cardType)!"" data=[{"id":"身份证", "name":"身份证"}, {"id":"其它", "name":"其它"}] class=validateClass/]
				   	[#else]
						<input name="${config.property.property}" value="${text(config.property.property)!}"
						       maxlength="${config.property.length!30}" [#if config.required]required[/#if] >
		            [/#if]
		        [#else]
		            [@propertyInfoValue config/]
		        [/#if]
			</div>
		</div>
	[/#if]
[/#macro]
